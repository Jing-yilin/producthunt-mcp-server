    "@types/node": "^22.0.0",
    "typescript": "^5.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jing-yilin/producthunt-mcp-server.git"
